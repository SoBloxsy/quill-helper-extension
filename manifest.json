{
    "manifest_version": 3,
    "name": "Quill Helper Extension",
    "version": "1.1",
    "description": "Intercepts fetch requests to show multiple-choice options.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://*.quill.org/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.quill.org/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "update_url": "https://iren.blue/updates.xml"
  }
  