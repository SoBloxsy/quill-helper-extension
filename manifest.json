{
  "manifest_version": 3,
  "name": "Quill Interceptor",
  "version": "1.0",
  "description": "Intercepts fetch requests to log multiple-choice options.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://*.quill.org/*"],
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["*://*.quill.org/*"],
          "js": ["content.js"],
          "run_at": "document_start"
      }
  ]
}
